UNITS NATURAL
#RESTART
FLUSH STRIDE=1

FCCUBIC ...
LABEL=fcc
SPECIES=1-256000
SWITCH={CUBIC D_0=1.2 D_MAX=1.5}
ALPHA=27
MORE_THAN1={SMAP R_0=0.45 D_0=0.0 A=8 B=8}
LOWMEM
... FCCUBIC

RESTRAINT ARG=fcc.morethan-1 AT=80000 KAPPA=0.0003 LABEL=res

PRINT STRIDE=10 ARG=fcc.*,res.bias FILE=pre-COLVAR

ENDPLUMED
